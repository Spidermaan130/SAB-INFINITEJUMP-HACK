-- [[ ğŸ” SIMPLE AGE VERIFICATION ]] --
local function QuickAgeCheck()
    game:GetService("StarterGui"):SetCore("SendNotification", {
        Title = T.age_title,
        Text = "Checking...",
        Duration = 3
    })
    
    wait(1)
    
    -- Simple check
    local isUnder5 = false
    local childWords = {"baby", "kid", "child", "toddler", "infant", "little"}
    for _, word in pairs(childWords) do
        if username:find(word) then
            isUnder5 = true
            break
        end
    end
    
    if isUnder5 then
        game:GetService("StarterGui"):SetCore("SendNotification", {
            Title = T.too_young,
            Text = T.too_young_msg,
            Duration = 5
        })
        return false
    end
    
    game:GetService("StarterGui"):SetCore("SendNotification", {
        Title = T.verified,
        Text = T.verified_msg,
        Duration = 3
    })
    
    return true
end

-- Run age check
