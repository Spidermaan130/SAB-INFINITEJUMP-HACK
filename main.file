local active = false
local JUMP_POWER = 34 

-- üõ°Ô∏è HEARTBEAT LOOP (Health & Anti-Cheat) üõ°Ô∏è
RunService.Heartbeat:Connect(function()
    if active then
        local char = p.Character
        local hum = char and char:FindFirstChildOfClass("Humanoid")
        local hrp = char and char:FindFirstChild("HumanoidRootPart")
        
        if hum and hrp then
            -- INFINITE HEALTH (God Mode)
            hum.MaxHealth = 99999999
            hum.Health = 99999999
            hum:SetStateEnabled(Enum.HumanoidStateType.Dead, false)
            
            -- NETWORK OWNERSHIP (Stops Rubberband)
            p.SimulationRadius = math.huge
            
            -- FAKE PHYSICS STATE
            if hum:GetState() == Enum.HumanoidStateType.NoPhysics then
                hum:ChangeState(Enum.HumanoidStateType.FallingDown)
            end
        end
    end
end)

-- üõ°Ô∏è MASSLESS ITEM SYSTEM üõ°Ô∏è
p.CharacterChildAdded:Connect(function(child)
    if active and child:IsA("Tool") then
        task.wait(0.1)
        for _, part in pairs(child:GetDescendants()) do
            if part:IsA("BasePart") then
                part.Massless = true 
                part.CanCollide = false
            end
        end
    end
end)

-- JUMP HANDLER
UIS.JumpRequest:Connect(function()
    if active then
        local char = p.Character
        local hrp = char and char:FindFirstChild("HumanoidRootPart")
        
        if hrp then
            local bv = Instance.new("BodyVelocity")
            bv.MaxForce = Vector3.new(0, 500000, 0)
            bv.Velocity = Vector3.new(0, JUMP_POWER, 0)
            bv.Parent = hrp
            
            task.wait(0.15) 
            
            -- Natural Decay (To fool server-side logs)
            for i = 1, 4 do
                bv.Velocity = bv.Velocity * 0.4
                task.wait(0.03)
            end
            bv:Destroy()
        end
    end
end)

-- [[ UI ACTIONS ]] --
toggleBtn.MouseButton1Click:Connect(function() mainHub.Visible = not mainHub.Visible end)

jumpBtn.MouseButton1Click:Connect(function()
    active = not active
    jumpBtn.Text = "GOD JUMP: " .. (active and "ON" or "OFF")
    jumpBtn.BackgroundColor3 = active and COLORS.SUCCESS or COLORS.ERROR
    
    -- Reset Health to 100 when turned off
    if not active then
        local hum = p.Character and p.Character:FindFirstChildOfClass("Humanoid")
        if hum then 
            hum:SetStateEnabled(Enum.HumanoidStateType.Dead, true)
            hum.MaxHealth = 100
            hum.Health = 100 
        end
    end
end)

-- RGB Spin Animation
RunService.RenderStepped:Connect(function()
    local r = tick() * 60
    if tg then tg.Rotation = r end
    if hg then hg.Rotation = r end
end)

-- Mobile Dragging Logic for Delta
local drag, dStart, sPos
toggleBtn.InputBegan:Connect(function(i) if i.UserInputType == Enum.UserInputType.Touch then drag = true; dStart = i.Position; sPos = toggleBtn.Position end end)
UIS.InputChanged:Connect(function(i) if drag and i.UserInputType == Enum.UserInputType.Touch then local delta = i.Position - dStart; toggleBtn.Position = UDim2.new(sPos.X.Scale, sPos.X.Offset + delta.X, sPos.Y.Scale, sPos.Y.Offset + delta.Y) end end)
UIS.InputEnded:Connect(function(i) if i.UserInputType == Enum.UserInputType.Touch then drag = false end end)
