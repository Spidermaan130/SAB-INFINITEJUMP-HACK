-- LOGIC
local active = false
local JUMP_POWER = 35 
local RunService = game:GetService("RunService")
local UIS = game:GetService("UserInputService")

UIS.JumpRequest:Connect(function()
    if active then
        local char = LP.Character
        local hrp = char and char:FindFirstChild("HumanoidRootPart")
        if hrp then
            -- STABLE JUMP: Uses Force instead of Velocity for smoother items-carrying
            local bv = Instance.new("BodyVelocity")
            bv.MaxForce = Vector3.new(0, 800000, 0)
            bv.Velocity = Vector3.new(0, JUMP_POWER, 0)
            bv.Parent = hrp
            task.wait(0.12) 
            bv:Destroy()
        end
    end
end)

