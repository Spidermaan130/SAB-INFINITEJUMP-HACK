})

-- [[ üõ°Ô∏è INFINITE JUMP PHYSICS ]] --
local UIS = game:GetService("UserInputService")
local RunService = game:GetService("RunService")

-- Jump function
local function PerformJump()
    if not InfiniteJumpEnabled then return end
    
    local char = localPlayer.Character
    if not char then return end
    
    local hrp = char:FindFirstChild("HumanoidRootPart")
    local humanoid = char:FindFirstChildOfClass("Humanoid")
    
    if hrp and humanoid and humanoid:GetState() ~= Enum.HumanoidStateType.Dead then
        -- BAC-3209 Bypass: Safe velocity change
        hrp.Velocity = Vector3.new(hrp.Velocity.X, JUMP_POWER, hrp.Velocity.Z)
    end
end

-- Connect events
UIS.JumpRequest:Connect(PerformJump)

-- Mobile touch
UIS.TouchTap:Connect(function(_, processed)
    if not processed and InfiniteJumpEnabled then
        PerformJump()
    end
end)

-- Auto-jump
local autoJump = RunService.Heartbeat:Connect(function()
    if InfiniteJumpEnabled then
        local char = localPlayer.Character
        if char then
            local humanoid = char:FindFirstChildOfClass("Humanoid")
            if humanoid and humanoid.FloorMaterial ~= Enum.Material.Air then
                PerformJump()
                task.wait(0.15)
            end
        end
    end
end)

-- Cleanup
JumpToggle:OnChanged(function(Value)
    InfiniteJumpEnabled = Value
    if not Value then
        autoJump:Disconnect()
    end
end)

