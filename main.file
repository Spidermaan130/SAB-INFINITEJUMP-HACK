-- [[ STYLING & ANIMATION HELPERS ]] --
local theme = {
    bg = Color3.fromRGB(25, 25, 30),
    accent1 = Color3.fromRGB(0, 200, 255), -- Cyan
    accent2 = Color3.fromRGB(200, 0, 255), -- Purple
    text = Color3.fromRGB(240, 240, 240),
