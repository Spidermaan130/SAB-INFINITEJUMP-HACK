-- [[ üõ†Ô∏è UTILITY FUNCTIONS ]] --
function CreateRoundedFrame(parent, size, pos, color)
    local f = Instance.new("Frame")
    f.Size = size
    f.Position = pos
    f.BackgroundColor3 = color
    f.BorderSizePixel = 0
    f.Parent = parent
    local c = Instance.new("UICorner")
    c.CornerRadius = UDim.new(0, 10)
    c.Parent = f
    return f
end

function ShowWelcome()
    local welcomeFrame = CreateRoundedFrame(ScreenGui, UDim2.new(0, 280, 0, 70), UDim2.new(1, 10, 0.1, 0), Color3.fromRGB(30, 30, 55))
    
    local img = Instance.new("ImageLabel")
    img.Size = UDim2.new(0, 50, 0, 50)
    img.Position = UDim2.new(0, 10, 0.5, -25)
    img.Image = "rbxthumb://type=AvatarHeadShot&id=" .. localPlayer.UserId .. "&w=150&h=150"
    img.BackgroundTransparency = 1
    img.Parent = welcomeFrame
    Instance.new("UICorner", img).CornerRadius = UDim.new(1, 0)

    local txt = Instance.new("TextLabel")
    txt.Size = UDim2.new(1, -70, 1, 0)
    txt.Position = UDim2.new(0, 65, 0, 0)
    txt.BackgroundTransparency = 1
    txt.Text = T.welcome .. localPlayer.Name .. "!"
    txt.TextColor3 = Color3.fromRGB(255, 255, 255)
    txt.Font = Enum.Font.GothamBold
    txt.TextSize = 14
    txt.TextWrapped = true
    txt.TextXAlignment = (CurrentLocale:find("ar") and Enum.TextXAlignment.Right or Enum.TextXAlignment.Left)
    txt.Parent = welcomeFrame

    welcomeFrame:TweenPosition(UDim2.new(1, -290, 0.1, 0), "Out", "Back", 0.6, true)
    task.delay(4, function()
        welcomeFrame:TweenPosition(UDim2.new(1, 10, 0.1, 0), "In", "Linear", 0.5, true)
        task.wait(0.5)
        welcomeFrame:Destroy()
    end)
end

-- [[ ‚è≥ 20-SECOND LOADING SYSTEM ]] --
function StartLoading()
    local loadFrame = Instance.new("Frame")
    loadFrame.Size = UDim2.new(1, 0, 1, 0)
    loadFrame.BackgroundColor3 = Color3.fromRGB(10, 10, 20)
    loadFrame.Parent = ScreenGui
    
    local barContainer = CreateRoundedFrame(loadFrame, UDim2.new(0, 300, 0, 15), UDim2.new(0.5, -150, 0.5, 0), Color3.fromRGB(40, 40, 50))
    local bar = CreateRoundedFrame(barContainer, UDim2.new(0, 0, 1, 0), UDim2.new(0, 0, 0, 0), Color3.fromRGB(0, 150, 255))
    
    local status = Instance.new("TextLabel")
    status.Size = UDim2.new(1, 0, 0, 40)
    status.Position = UDim2.new(0, 0, 0.4, 0)
    status.Text = T.loading
    status.TextColor3 = Color3.fromRGB(200, 200, 255)
    status.Font = Enum.Font.GothamBold
    status.BackgroundTransparency = 1
    status.Parent = loadFrame

    for i = 1, 100 do
        task.wait(0.2) -- Total 20 Seconds
        bar.Size = UDim2.new(i/100, 0, 1, 0)
        status.Text = T.loading .. " " .. i .. "%"
    end
    loadFrame:Destroy()
    ShowKeySystem()
end

