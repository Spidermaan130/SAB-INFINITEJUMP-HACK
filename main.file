function ShowKeySystem()
    local keyWindow = CreateRoundedFrame(ScreenGui, UDim2.new(0, 350, 0, 220), UDim2.new(0.5, -175, 0.5, -110), Color3.fromRGB(20, 20, 35))
    
    local title = Instance.new("TextLabel")
