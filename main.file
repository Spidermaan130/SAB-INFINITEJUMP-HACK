-- SAB INFINITE JUMP SYSTEM (FIXED - NO FALL DAMAGE)
local jumpConnection = nil

local function setupSABJump()
    local function onJumpRequest()
        if not infJump then return end
        
        local character = p.Character
        if not character then return end
        
        local humanoid = character:FindFirstChildOfClass("Humanoid")
        local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
        
        if humanoid and humanoidRootPart and humanoid.Health > 0 then
            -- SAB METHOD: Reset fall-damage timer
            -- 1. Reset vertical velocity to 0
            humanoidRootPart.Velocity = Vector3.new(
                humanoidRootPart.Velocity.X,
                0,
                humanoidRootPart.Velocity.Z
            )
            
            -- 2. Force Landing state to reset fall timer
            humanoid:ChangeState(Enum.HumanoidStateType.Landing)
            
            -- 3. Brief wait for state to register
            task.wait(0.01)
            
            -- 4. Force Jumping state
            humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
            
            -- 5. Apply upward velocity for consistent jump height
            humanoidRootPart.Velocity = Vector3.new(
                humanoidRootPart.Velocity.X,
                50, -- Optimal jump height
                humanoidRootPart.Velocity.Z
            )
            
            -- 6. Cooldown to prevent spam
            task.wait(0.08)
        end
    end
    
    -- Connect to JumpRequest
    jumpConnection = UserInputService.JumpRequest:Connect(onJumpRequest)
    
    -- Handle character respawns
    p.CharacterAdded:Connect(function()
        task.wait(0.5) -- Wait for character to load
        
        if jumpConnection then
            jumpConnection:Disconnect()
        end
        
        jumpConnection = UserInputService.JumpRequest:Connect(onJumpRequest)
    end)
    
    p.CharacterRemoving:Connect(function()
        if jumpConnection then
            jumpConnection:Disconnect()
            jumpConnection = nil
        end
    end)
end

-- Setup jump system immediately
setupSABJump()

-- Animation Functions
local function animateHover(object, isHovering)
    local scale = isHovering and 1.05 or 1
    local transparency = isHovering and 0.8 : COLORS.GLASS_TRANSPARENCY
    
    TweenService:Create(object, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
        Size = object.Size * scale
    }):Play()
    
    if object:IsA("TextButton") or object:IsA("TextBox") then
        TweenService:Create(object, TweenInfo.new(0.2), {
            BackgroundTransparency = transparency
        }):Play()
    end
end

local function animateClick(object)
    local originalSize = object.Size
    
    TweenService:Create(object, TweenInfo.new(0.1, Enum.EasingStyle.Back, Enum.EasingDirection.In), {
        Size = object.Size * 0.95
    }):Play()
    
    task.wait(0.1)
    
    TweenService:Create(object, TweenInfo.new(0.2, Enum.EasingStyle.Elastic, Enum.EasingDirection.Out), {
        Size = originalSize
    }):Play()
end

-- Pulsating toggle button animation
local function startTogglePulse()
    task.spawn(function()
        while toggleButton and toggleButton.Parent do
            TweenService:Create(toggleButton, TweenInfo.new(1.5, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut), {
                Size = UDim2.new(0, 90, 0, 90)
            }):Play()
            
            TweenService:Create(toggleGlow, TweenInfo.new(1.5, Enum.EasingStyle.Sine), {
                ImageTransparency = 0.4
            }):Play()
            
            task.wait(1.5)
            
            TweenService:Create(toggleButton, TweenInfo.new(1.5, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut), {
                Size = UDim2.new(0, 85, 0, 85)
            }):Play()
            
            TweenService:Create(toggleGlow, TweenInfo.new(1.5, Enum.EasingStyle.Sine), {
                ImageTransparency = 0.6
            }):Play()
            
            task.wait(1.5)
        end
    end)
end

-- Show notification
local function showNotification(message)
    notificationLabel.Text = message
    
    TweenService:Create(notificationFrame, TweenInfo.new(0.5, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {
        Position = UDim2.new(0.15, 0, 0.02, 0),
        BackgroundTransparency = COLORS.GLASS_TRANSPARENCY - 0.1
    }):Play()
    
    TweenService:Create(notificationLabel, TweenInfo.new(0.3), {
        TextTransparency = 0
    }):Play()
    
    notificationFrame.Visible = true
    
    task.wait(3)
    
    TweenService:Create(notificationFrame, TweenInfo.new(0.5, Enum.EasingStyle.Back, Enum.EasingDirection.In), {
        Position = UDim2.new(0.15, 0, -0.1, 0),
        BackgroundTransparency = COLORS.GLASS_TRANSPARENCY
    }):Play()
    
    TweenService:Create(notificationLabel, TweenInfo.new(0.3), {
        TextTransparency = 1
    }):Play()
    
    task.wait(0.5)
    notificationFrame.Visible = false
end

