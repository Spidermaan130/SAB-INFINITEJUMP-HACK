            ShowMainHub()
        else
            input.Text = "WRONG KEY"
            task.wait(1)
            input.Text = ""
        end
    end)
end

function ShowMainHub()
    local main = Instance.new("Frame")
    main.Size = UDim2.new(0, 360, 0, 280)
    main.Position = UDim2.new(0.5, -180, 0.5, -140)
