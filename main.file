    title.TextColor3 = Color3.fromRGB(255, 255, 255)
    title.Font = Enum.Font.GothamBold
    title.BackgroundTransparency = 1
    title.Parent = keyWindow

    local input = Instance.new("TextBox")
    input.Size = UDim2.new(0, 250, 0, 45)
    input.Position = UDim2.new(0.5, -125, 0.4, 0)
    input.BackgroundColor3 = Color3.fromRGB(40, 40, 60)
    input.PlaceholderText = "..."
    input.Text = ""
    input.TextColor3 = Color3.fromRGB(255, 255, 255)
    input.Parent = keyWindow
    Instance.new("UICorner", input).CornerRadius = UDim.new(0, 8)

    local submit = Instance.new("TextButton")
    submit.Size = UDim2.new(0, 150, 0, 40)
    submit.Position = UDim2.new(0.5, -75, 0.75, 0)
    submit.BackgroundColor3 = Color3.fromRGB(0, 120, 255)
    submit.Text = T.submit
    submit.TextColor3 = Color3.fromRGB(255, 255, 255)
    submit.Font = Enum.Font.GothamBold
    submit.Parent = keyWindow
    Instance.new("UICorner", submit).CornerRadius = UDim.new(0, 8)

    submit.MouseButton1Click:Connect(function()
        if input.Text:upper() == CorrectKey then
            keyWindow:Destroy()
            ShowWelcome()
            ShowMainHub()
        else
            input.Text = ""
            input.PlaceholderText = "‚ùå INVALID"
        end
    end)
end

