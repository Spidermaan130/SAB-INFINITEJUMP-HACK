-- [[ üåê MANUAL LANGUAGE MENU ]] --
function ShowLanguageMenu(parentHub)
    local menu = CreateRoundedFrame(parentHub, UDim2.new(1, 0, 1, 0), UDim2.new(0, 0, 0, 0), Color3.fromRGB(15, 15, 30))
    
    local scroll = Instance.new("ScrollingFrame")
    scroll.Size = UDim2.new(1, -20, 0.8, 0)
    scroll.Position = UDim2.new(0, 10, 0.1, 0)
    scroll.BackgroundTransparency = 1
    scroll.CanvasSize = UDim2.new(0, 0, 3, 0)
    scroll.ScrollBarThickness = 4
    scroll.Parent = menu

    local list = Instance.new("UIListLayout")
    list.Padding = UDim.new(0, 5)
    list.Parent = scroll

    for code, data in pairs(Translations) do
        local b = Instance.new("TextButton")
        b.Size = UDim2.new(1, 0, 0, 35)
        b.BackgroundColor3 = Color3.fromRGB(40, 40, 70)
        b.Text = code:upper()
        b.TextColor3 = Color3.fromRGB(255, 255, 255)
        b.Font = Enum.Font.Gotham
        b.Parent = scroll
        Instance.new("UICorner", b)
        
        b.MouseButton1Click:Connect(function()
            T = Translations[code]
            CurrentLocale = code
            menu:Destroy()
            parentHub:Destroy()
            ShowMainHub() -- Refresh UI
        end)
    end

    local close = Instance.new("TextButton")
    close.Size = UDim2.new(0, 100, 0, 30)
    close.Position = UDim2.new(0.5, -50, 0.9, 0)
    close.Text = "CLOSE"
    close.Parent = menu
    close.MouseButton1Click:Connect(function() menu:Destroy() end)
end

