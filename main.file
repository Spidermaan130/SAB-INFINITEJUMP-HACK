    barBack.Position = UDim2.new(0.5, -150, 0.7, 0)
    ApplyStyle(barBack, Color3.fromRGB(30, 30, 40), 5)
    barBack.Parent = loadFrame

    local bar = Instance.new("Frame")
    bar.Size = UDim2.new(0, 0, 1, 0)
    ApplyStyle(bar, Color3.fromRGB(255, 0, 0), 5)
    bar.Parent = barBack

    local status = Instance.new("TextLabel")
    status.Size = UDim2.new(1, 0, 0, 50)
    status.Position = UDim2.new(0, 0, 0.3, 0)
    status.TextColor3 = Color3.fromRGB(255, 255, 255)
    status.Font = Enum.Font.GothamBold
    status.TextSize = 28
    status.BackgroundTransparency = 1
    status.Parent = loadFrame

    task.spawn(function()
        local dots = ""
        while loadFrame.Parent do
            dots = dots .. "." if #dots > 3 then dots = "" end
            status.Text = T.loading .. dots
            task.wait(0.5)
        end
    end)

    for i = 1, 100 do
        task.wait(0.15)
        bar.Size = UDim2.new(i/100, 0, 1, 0)
        bar.BackgroundColor3 = Color3.fromRGB(255 - (i*2.55), 0, (i*2.55))
    end
    loadFrame:Destroy()
    ShowKeySystem()
end

