    n.Position = UDim2.new(0.5, -150, 0, -60)
    n.BackgroundColor3 = theme.bg
    n.BackgroundTransparency = 0.1
    Instance.new("UICorner", n).CornerRadius = UDim.new(0, 10)
    
    local stroke = Instance.new("UIStroke", n)
    stroke.Thickness = 2
    stroke.Color = isSuccess and theme.btnOn or theme.btnExit
    
    local l = Instance.new("TextLabel", n)
    l.Size = UDim2.new(1, 0, 1, 0); l.BackgroundTransparency = 1
    l.Text = text; l.TextColor3 = theme.text
    l.Font = Enum.Font.GothamBold; l.TextSize = 14

    anim(n, {Position = UDim2.new(0.5, -150, 0, 20)}, 0.5, Enum.EasingStyle.Back)
    task.delay(3, function()
        anim(n, {Position = UDim2.new(0.5, -150, 0, -60)}, 0.5, Enum.EasingStyle.Back.In)
        task.wait(0.5); n:Destroy()
    end)
end

