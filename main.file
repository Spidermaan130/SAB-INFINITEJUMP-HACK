    end)

    for i = 1, 100 do
        task.wait(0.2)
        bar.Size = UDim2.new(i/100, 0, 1, 0)
        
        -- Color Transition
        local colorR = 255 - (i * 2.55)
        local colorB = i * 2.55
        bar.BackgroundColor3 = Color3.fromRGB(colorR, 0, colorB)
        
