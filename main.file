-- [[ ðŸ› ï¸ BEAUTIFY UTILS ]] --
local function ApplyStyle(obj, color, radius)
    obj.BackgroundColor3 = color
    obj.BorderSizePixel = 0
    local c = Instance.new("UICorner")
    c.CornerRadius = UDim.new(0, radius or 12)
    c.Parent = obj
end

local function CreateShadow(parent)
    local s = Instance.new("ImageLabel")
    s.AnchorPoint = Vector2.new(0.5, 0.5)
    s.Position = UDim2.new(0.5, 0, 0.5, 0)
    s.Size = UDim2.new(1, 40, 1, 40)
    s.BackgroundTransparency = 1
    s.Image = "rbxassetid://6015667341"
    s.ImageColor3 = Color3.fromRGB(0, 0, 0)
    s.ImageTransparency = 0.5
    s.ZIndex = parent.ZIndex - 1
    s.Parent = parent
end

-- [[ ðŸ”‡ SOUND LOOP CONTROLLER ]] --
local loopSound

function ShowWarning()
    loopSound = Instance.new("Sound")
    loopSound.SoundId = ALERT_SOUND_ID
    loopSound.Looped = true -- Loops until warning is done
    loopSound.Parent = game.Workspace
    loopSound:Play()

    local warnFrame = Instance.new("Frame")
    warnFrame.Size = UDim2.new(0, 420, 0, 200)
    warnFrame.Position = UDim2.new(0.5, -210, 0.5, -100)
    ApplyStyle(warnFrame, Color3.fromRGB(35, 5, 5), 20)
    warnFrame.Parent = ScreenGui
    CreateShadow(warnFrame)

    local stroke = Instance.new("UIStroke")
    stroke.Thickness = 3
    stroke.Color = Color3.fromRGB(255, 0, 0)
    stroke.Parent = warnFrame

    local msg = Instance.new("TextLabel")
    msg.Size = UDim2.new(1, -40, 1, -40)
    msg.Position = UDim2.new(0, 20, 0, 20)
    msg.BackgroundTransparency = 1
    msg.Text = T.warn
    msg.TextColor3 = Color3.fromRGB(255, 255, 255)
    msg.Font = Enum.Font.GothamBold
    msg.TextSize = 22
    msg.TextWrapped = true
    msg.Parent = warnFrame

    task.wait(6) 
    loopSound:Stop()
    warnFrame:Destroy()
    ShowSplashScreen()
end

function ShowSplashScreen()
    local splash = Instance.new("TextLabel")
    splash.Size = UDim2.new(1, 0, 0, 100)
    splash.Position = UDim2.new(0, 0, 0.5, -50)
