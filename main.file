    title.BackgroundTransparency = 1
    title.Parent = keyWin

    local input = Instance.new("TextBox")
    input.Size = UDim2.new(0, 320, 0, 55)
    input.Position = UDim2.new(0.5, -160, 0.4, 0)
    input.PlaceholderText = T.key
    input.Text = ""
    ApplyStyle(input, Color3.fromRGB(25, 25, 35), 15)
    input.TextColor3 = Color3.fromRGB(255, 255, 255)
    input.Font = Enum.Font.Gotham
    input.TextSize = 18
    input.Parent = keyWin

    local submit = Instance.new("TextButton")
    submit.Size = UDim2.new(0, 220, 0, 55)
    submit.Position = UDim2.new(0.5, -110, 0.7, 10)
    submit.Text = T.submit
    submit.TextColor3 = Color3.fromRGB(255, 255, 255)
    submit.Font = Enum.Font.GothamBold
    submit.TextSize = 20
    ApplyStyle(submit, Color3.fromRGB(0, 100, 255), 15)
