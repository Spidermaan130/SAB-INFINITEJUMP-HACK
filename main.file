-- [[ üõ†Ô∏è UTILS ]] --
local function ApplyStyle(obj, color, radius)
    obj.BackgroundColor3 = color
    obj.BorderSizePixel = 0
    local c = Instance.new("UICorner")
    c.CornerRadius = UDim.new(0, radius or 12)
    c.Parent = obj
end

local function MakeDraggable(topbar, obj)
    local dragging, inputStart, startPos
    topbar.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
            dragging = true inputStart = input.Position startPos = obj.Position
            input.Changed:Connect(function() if input.UserInputState == Enum.UserInputState.End then dragging = false end end)
        end
    end)
    UIS.InputChanged:Connect(function(input)
        if dragging and (input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch) then
            local delta = input.Position - inputStart
            obj.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
        end
    end)
end

-- [[ üîá SOUND CONTROLLER ]] --
local function MuteGame()
    for _, sound in pairs(game:GetDescendants()) do
        if sound:IsA("Sound") and sound.Playing then
            StoredVolumes[sound] = sound.Volume
            sound.Volume = 0
        end
    end
end

local function UnmuteGame()
    for sound, originalVolume in pairs(StoredVolumes) do
        if sound and sound.Parent then
            sound.Volume = originalVolume
        end
    end
    StoredVolumes = {}
end

