-- Create rotating RGB gradient
local function createRGBGradient()
    local gradient = Instance.new("UIGradient")
    gradient.Color = ColorSequence.new{
        ColorSequenceKeypoint.new(0, COLORS.ACCENT_PINK),
        ColorSequenceKeypoint.new(0.33, COLORS.ACCENT_CYAN),
        ColorSequenceKeypoint.new(0.66, COLORS.ACCENT_PURPLE),
        ColorSequenceKeypoint.new(1, COLORS.ACCENT_PINK)
    }
    gradient.Rotation = 0
    return gradient
end

-- Create gradient stroke
local function createGradientStroke(parent, thickness)
    local stroke = Instance.new("UIStroke")
    stroke.Thickness = thickness
    stroke.LineJoinMode = Enum.LineJoinMode.Round
    stroke.Transparency = 0
    local gradient = createRGBGradient()
    gradient.Parent = stroke
    stroke.Parent = parent
    return stroke, gradient
end

