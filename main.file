-- [[ üîû AGE VERIFICATION QUESTIONS ]] --
local function AgeVerification()
    game:GetService("StarterGui"):SetCore("SendNotification", {
        Title = lang.age_check,
        Text = "Answer 5 questions...",
        Duration = 3
    })
    
    task.wait(2)
    
    local score = 0
    
    for i = 1, 5 do
        game:GetService("StarterGui"):SetCore("SendNotification", {
            Title = lang.question .. " " .. i,
            Text = lang.questions[i],
            Duration = 4
        })
        
        task.wait(3)
        
        -- Simulate answer (in real script, you'd wait for user input)
        -- For now, give 80% chance of correct answer
        if math.random(1, 100) <= 80 then
            score = score + 1
            game:GetService("StarterGui"):SetCore("SendNotification", {
                Title = lang.correct,
                Text = "Score: " .. score .. "/5",
                Duration = 2
            })
        else
            game:GetService("StarterGui"):SetCore("SendNotification", {
                Title = lang.wrong,
                Text = "Score: " .. score .. "/5",
                Duration = 2
            })
        end
        
        task.wait(1)
    end
    
    -- Check if passed (need at least 3/5 correct)
    if score >= 3 then
        game:GetService("StarterGui"):SetCore("SendNotification", {
            Title = lang.passed,
            Text = "Score: " .. score .. "/5",
            Duration = 4
        })
        return true
    else
        game:GetService("StarterGui"):SetCore("SendNotification", {
            Title = lang.failed,
            Text = "Score: " .. score .. "/5 - Blocked",
            Duration = 5
        })
        return false
    end
end

-- Run age verification
