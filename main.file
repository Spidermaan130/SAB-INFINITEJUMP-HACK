    stroke.Thickness = 4
    stroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
    stroke.Parent = keyWin

    -- Border Rainbow Loop
    task.spawn(function()
        while keyWin.Parent do
            stroke.Color = Color3.fromHSV((tick()%4)/4, 0.8, 1)
            task.wait()
        end
    end)

    local title = Instance.new("TextLabel")
    title.Size = UDim2.new(1, 0, 0, 60)
