-- Function to physically disable a method so it cannot be called
local function DisableMethod(obj, name)
    local success, original = pcall(function() return obj[name] end)
    if success and type(original) == "function" then
        hookfunction(original, function(...) 
            warn("üõ°Ô∏è SHIELD: Prevented " .. name .. " from running!")
            return nil 
        end)
    end
end

-- Completely kill all ways the game can remove or move you
DisableMethod(LP, "Kick")
DisableMethod(LP, "kick")
DisableMethod(TeleportService, "Teleport")
DisableMethod(TeleportService, "TeleportAsync")
DisableMethod(TeleportService, "TeleportToPlaceInstance")
DisableMethod(TeleportService, "TeleportToPrivateServer")
DisableMethod(TeleportService, "TeleportToSpawnPage")

-- Metatable backup for safety
local mt = getrawmetatable(game)
local old = mt.__namecall
setreadonly(mt, false)
mt.__namecall = newcclosure(function(self, ...)
    local method = getnamecallmethod()
    if method == "Kick" or method == "kick" or method:find("Teleport") then
        return nil
    end
    return old(self, ...)
end)
setreadonly(mt, true)

