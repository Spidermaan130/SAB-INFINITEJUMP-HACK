    local title = Instance.new("TextLabel")
    title.Size = UDim2.new(1, 0, 0, 60)
    title.Text = T.title
    title.Font = Enum.Font.GothamBlack
    title.TextSize = 22
    title.BackgroundTransparency = 1
    title.Parent = main

    task.spawn(function()
        while main.Parent do
            title.TextColor3 = Color3.fromHSV((tick()%5)/5, 0.8, 1)
            task.wait()
        end
    end)

    local jumpBtn = Instance.new("TextButton")
    jumpBtn.Size = UDim2.new(1, -40, 0, 60)
    jumpBtn.Position = UDim2.new(0, 20, 0, 80)
    jumpBtn.Text = T.jump .. ": OFF"
    jumpBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
    jumpBtn.Font = Enum.Font.GothamBold
    ApplyStyle(jumpBtn, Color3.fromRGB(40, 40, 50), 12)
    jumpBtn.Parent = main

    jumpBtn.MouseButton1Click:Connect(function()
        InfiniteJumpEnabled = not InfiniteJumpEnabled
        jumpBtn.Text = T.jump .. ": " .. (InfiniteJumpEnabled and "ON" or "OFF")
        jumpBtn.BackgroundColor3 = InfiniteJumpEnabled and Color3.fromRGB(0, 180, 100) or Color3.fromRGB(40, 40, 50)
    end)
end

