-- Modern UI Styling
local function style(obj, color, round)
    local c = Instance.new("UICorner", obj)
    c.CornerRadius = UDim.new(0, round or 10)
    if obj:IsA("Frame") then
        obj.BackgroundColor3 = color
        local s = Instance.new("UIStroke", obj)
        s.Color = Color3.fromRGB(0, 150, 255)
        s.Thickness = 2
    end
end

local function buildWindow(size, pos)
    local f = Instance.new("Frame", SG)
    f.Size = size f.Position = pos
    style(f, Color3.fromRGB(35, 0, 0))
    local h = Instance.new("TextLabel", f)
    h.Size = UDim2.new(1,0,0,45) h.Text = HEADER_TEXT
    h.TextColor3 = Color3.fromRGB(255, 255, 255)
    h.Font = Enum.Font.GothamBold h.TextSize = 16 h.BackgroundTransparency = 1
    local img = Instance.new("ImageLabel", f)
    img.Size = UDim2.new(0,45,0,45) img.Position = UDim2.new(0,10,0,50)
    img.Image = IMG_ID img.BackgroundTransparency = 1
    Instance.new("UICorner", img).CornerRadius = UDim.new(1,0)
    return f, h
end

