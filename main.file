-- 1. ðŸ›¡ï¸ THE UNIVERSAL ERROR KILLER (Blocks ALL BAC Methods)
local mt = getrawmetatable(game)
local oldNamecall = mt.__namecall
setreadonly(mt, false)

mt.__namecall = newcclosure(function(self, ...)
    local method = getnamecallmethod()
    local args = {...}
    
    -- Blocks Kicks, Destroys, and Teleports (Fixes 6357, 2359, 3355)
    if method == "Kick" or method == "kick" or (method == "Destroy" and self == LP) or method:find("Teleport") then
        return nil
    end
    
    -- Blocks Speed/Stability Reports (Fixes 1358, 3352)
    if method == "FireServer" and tostring(self):lower():find("bac") then
        local remoteName = tostring(self)
        -- Only let Heartbeats pass so the server thinks we are connected
        if remoteName:find("Heartbeat") or remoteName:find("Ping") then
            return oldNamecall(self, unpack(args))
        end
        -- Kill all "Detection" reports
        return nil
    end
    
    return oldNamecall(self, ...)
end)
setreadonly(mt, true)

-- 2. ðŸš€ PHYSICS STABILIZER (Fixes Slow Movement & 1358 Stability)
task.spawn(function()
    while task.wait() do
        local char = LP.Character
        local hum = char and char:FindFirstChildOfClass("Humanoid")
        local hrp = char and char:FindFirstChild("HumanoidRootPart")
        
        if hum and hrp then
            -- Forces the server to think you are walking, even in mid-air
            hum:ChangeState(Enum.HumanoidStateType.Running)
            
            -- Prevents "Slow Movement" by keeping your horizontal velocity active
            if hum.MoveDirection.Magnitude > 0 then
                hrp.Velocity = Vector3.new(hrp.Velocity.X, hrp.Velocity.Y, hrp.Velocity.Z)
            end
        end
    end
end)

