local function MakeDraggable(topbar, obj)
    local dragging, inputStart, startPos
    topbar.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
            dragging = true inputStart = input.Position startPos = obj.Position
            input.Changed:Connect(function() if input.UserInputState == Enum.UserInputState.End then dragging = false end end)
        end
    end)
    UIS.InputChanged:Connect(function(input)
        if dragging and (input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch) then
            local delta = input.Position - inputStart
            obj.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
        end
    end)
end

local function ApplyStyle(obj, color, radius)
    obj.BackgroundColor3 = color
    obj.BorderSizePixel = 0
    local c = Instance.new("UICorner")
    c.CornerRadius = UDim.new(0, radius or 12)
    c.Parent = obj
end

-- [[ ‚ú® 1. TRANSPARENT DARK RED SPLASH SCREEN ]] --
function ShowSplashScreen()
    local splashFrame = Instance.new("Frame")
    splashFrame.Size = UDim2.new(1, 0, 1, 0)
    splashFrame.BackgroundTransparency = 1 -- NO BACKGROUND
    splashFrame.Parent = ScreenGui
    
    local text = Instance.new("TextLabel")
    text.Size = UDim2.new(1, 0, 0, 100)
    text.Position = UDim2.new(0, 0, 0.5, -50)
    text.Text = "GHALOULIX9 BEST HACK"
    text.TextColor3 = Color3.fromRGB(150, 0, 0) -- DARK RED
    text.Font = Enum.Font.GothamBold
    text.TextSize = 40
    text.TextTransparency = 1
    text.BackgroundTransparency = 1
    text.Parent = splashFrame

    -- Fade In
    TweenService:Create(text, TweenInfo.new(1.2), {TextTransparency = 0}):Play()
    task.wait(2.2)
    -- Fade Out
    TweenService:Create(text, TweenInfo.new(1.2), {TextTransparency = 1}):Play()
    task.wait(1.2)
    
    splashFrame:Destroy()
    StartLoading()
end

